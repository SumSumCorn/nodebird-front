{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "esmaVersion": 2020,
    "sourceType": "module",
    "esmaFeatures": {
      "jsx": true
    },
    "requireConfigFile": false,
    "babelOptions": {
      "presets": ["@babel/preset-react"]
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "import",
    "react-hooks"
  ],
  "rules": {

  }
}
